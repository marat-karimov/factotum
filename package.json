{
  "name": "factotum",
  "productName": "Factotum",
  "version": "1.2.1",
  "description": "SQL powered tabular data viewer and editor",
  "main": ".webpack/main",
  "scripts": {
    "start": "NODE_ENV=development npm run server -- $ENGINE & electron-forge start",
    "pack": "electron-forge package && electron-builder --dir -c.mac.identity=null --publish never --config electron-builder.config.js",
    "dist": "electron-forge package && electron-builder --publish never --config electron-builder.config.js",
    "lint": "eslint --ext .ts,.tsx .",
    "setup-micromamba": "ts-node setup-micromamba.ts",
    "setup-venv": "./micromamba/bin/micromamba create --prefix ./micromamba/envs/venv -f environment.yml -y",
    "update-venv": "./micromamba/bin/micromamba update --prefix ./micromamba/envs/venv -f environment.yml -y",
    "server": "run-script-os",
    "server:test": "run-script-os",
    "server:win32": "./micromamba/envs/venv/python main.py",
    "server:nix": "./micromamba/envs/venv/bin/python main.py",
    "server:test:nix": "./micromamba/envs/venv/bin/pytest -v -s",
    "server:test:win32": "./micromamba/envs/venv/Scripts/pytest -v -s",
    "server:build": "./micromamba/bin/micromamba run --prefix ./micromamba/envs/venv python setup.py build",
    "test:e2e": "playwright test",
    "test:api": "NODE_ENV=test jest"
  },
  "keywords": [],
  "author": {
    "name": "Marat Karimov",
    "email": "karmagen88@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.2.1",
    "@electron-forge/plugin-auto-unpack-natives": "^6.2.1",
    "@electron-forge/plugin-webpack": "^6.2.1",
    "@playwright/test": "^1.36.2",
    "@types/codemirror": "^5.60.8",
    "@types/electron-localshortcut": "^3.1.0",
    "@types/jest": "^29.5.3",
    "@types/mousetrap": "^1.6.11",
    "@types/tabulator-tables": "^5.5.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "css-loader": "^6.0.0",
    "dotenv": "^16.3.1",
    "electron": "^24.3.0",
    "electron-builder": "^24.6.3",
    "electron-playwright-helpers": "^1.6.0",
    "eslint": "^8.0.1",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.25.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "glob": "^10.3.3",
    "jest": "^29.6.2",
    "node-loader": "^2.0.0",
    "playwright-core": "^1.36.2",
    "run-script-os": "^1.1.6",
    "style-loader": "^3.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@types/supertest": "^2.0.12",
    "axios": "^1.4.0",
    "codemirror": "5.65.13",
    "custom-electron-titlebar": "^4.2.3",
    "electron-localshortcut": "^3.2.1",
    "electron-squirrel-startup": "^1.0.0",
    "mousetrap": "^1.6.5",
    "tabulator-tables": "^5.5.0"
  }
}
